<div class="mainContent">
  <div class="content">
    <article class="topContent" id="faq">
      <div class="page_top">
        <div class="row" id="page-top-row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="icon">
			<p id="notice"><%= notice %></p>

			<p>
			  <strong>First name:</strong>
			  <%= @user.first_name %>
			</p>

			<p>
			  <strong>Last name:</strong>
			  <%= @user.last_name %>
			</p>

			<p>
			  <strong>Email address:</strong>
			  <%= @user.email %>
			</p>


			<%= link_to 'Edit', edit_user_path(@user) %> |
			<%= link_to 'Back', users_path %>
          </div>
        </div>
      </div>
    </article>
  </div>
</div>
